{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build:cjs", "^build:esm", "^build"],
			"outputs": ["cjs/**", "esm/**", "/**", ".next/**", "!.next/cache/**"],
			"cache": false
		},
		"build:cjs": {
			"dependsOn": ["^build:cjs"],
			"outputs": ["cjs/**"]
		},
		"build:esm": {
			"dependsOn": ["^build:esm"],
			"outputs": ["esm/**"]
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"husky:pre-push": {
			"outputMode": "errors-only",
			"dependsOn": ["lint"],
			"cache": false
		},
		"lint": {
			"cache": false
		},
		"dev": {
			"cache": true,
			"persistent": true
		},
		"start": {
			"dependsOn": ["^build"],
			"cache": false
		},
		"clean": {
			"cache": false
		}
	}
}
