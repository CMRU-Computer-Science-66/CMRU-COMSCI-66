{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"rootDir": "./src",
		"outDir": "./dist",
		"allowJs": true,
		"typeRoots": [
			"node_modules/@types"
		]
	},
	"plugins": [
		{
			"transform": "typescript-transform-paths"
		},
		{
			"transform": "typescript-transform-paths",
			"afterDeclarations": true
		}
	],
	"display": "cmru-comsci-66-database",
	"extends": "../tsconfig.base.json",
	"include": [
		"src"
	],
	"ts-node": {
		"require": [
			"typescript-transform-paths/register"
		]
	}
}
