{
	"private": false,
	"name": "@cmru-comsci-66/database",
	"version": "1.0.0",
	"license": "apache-2.0",
	"author": "Faelayis <48393914+Faelayis@users.noreply.github.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/Faelayis/CMRU-COMSCI-66.git",
		"directory": "library/database"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./lib-esm/core.js",
			"require": "./dist/core.js"
		}
	},
	"module": "lib-esm/core.js",
	"files": [
		"dist",
		"lib",
		"lib-esm",
		"prisma"
	],
	"scripts": {
		"build:esm": "tsc -m es6 --outDir lib-esm",
		"build:node": "tsc --build",
		"ci:bump": "bump",
		"ci:delete_scripts": "pnpm pkg delete scripts",
		"clean": "rimraf dist .turbo lib lib-esm tsconfig.tsbuildinfo prisma/client",
		"dev": "dotenv -e ../../.env -- prisma studio",
		"generate": "dotenv -e ../../.env -- prisma generate",
		"postinstall": "pnpm run --silent generate && pnpm --silent run \"/^build:.*/\"",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prisma:generate": "prisma generate",
		"pull": "dotenv -e ../../.env -- prisma db pull",
		"push": "dotenv -e ../../.env -- prisma db push"
	},
	"typings": "./lib-esm/core.d.ts",
	"dependencies": {
		"@prisma/client": "5.0.0",
		"prisma": "5.0.0"
	},
	"devDependencies": {
		"@types/node": "20.4.5",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"typescript-transform-paths": "3.4.6"
	},
	"publishConfig": {
		"@faelayis:registry": "https://npm.pkg.github.com",
		"access": "public",
		"linkDirectory": false
	},
	"prisma": {
		"schema": "prisma/schema.prisma"
	}
}
